use_frameworks!
platform :ios, '7.0'
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'FirebaseUI'
pod 'Firebase/Core'
pod 'Firebase/Storage'

target 'iMoney1.0' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  # use_frameworks!
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'FirebaseUI'
pod 'Firebase/Core'
pod 'Firebase/Storage'

  # Pods for iMoney1.0

  target 'iMoney1.0Tests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'FirebaseUI'
pod 'Firebase/Core'
pod 'Firebase/Storage'
  end

  target 'iMoney1.0UITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'FirebaseUI'
pod 'Firebase/Core'
pod 'Firebase/Storage'
  end

end
